if [[ -f $HOME/.alias ]]; then
  # Load local aliases
  source $HOME/.alias
fi

# Git users
alias gur8='git config user.name "Sergey Storchay" && git config user.email "r8@r8.com.ua"'
alias gumm='git config user.name "Sergey Storchay" && git config user.email "magemaven@gmail.com"'

# mc
alias mcab='mc -a -b'
alias mca='mc -a'

# Vagrant
alias vup='vagrant up --no-provision'
